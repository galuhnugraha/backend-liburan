<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">

  <div class="row">
    <% for(let x = 0; x < news.imageId.length; x++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img src="https://admin-liburan.herokuapp.com/<%= news.imageId[x].imageUrl %>" alt="image"
            style="width : 100%; display:'block'">
        </div>
      </div>
    </div>

    <% } %>
  </div>
  <a href="/admin/news" class="btn btn-warning">Back</a>
</div>